import * from "../../test"

push_scope("primitives")

test("number", fn {
    let a = 10

    expect(typeof(a) == number, "Expected a to be 'number'")
})

test("string", fn {
    let b = "string"
    expect(typeof(b) == string, "Expected b to be 'string'")
})
    
test("bool", fn {
    let c = false
    expect(typeof(c) == bool, "Expected c to be 'bool'")
})
    
test("null", fn {
    let d = null
    expect(typeof(d) == type(null), "Expected d to be 'null'")
})

test("enum", fn {
    type TestEnum = enum { A, B, C }
    
    let e = TestEnum.A

    expect(typeof(e) == TestEnum, "Expected e to be 'TestEnum'")
})

pop_scope()