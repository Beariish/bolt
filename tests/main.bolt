import core
import print from core
import mod

fn test_fn(x: number, y: number) {
    return x + y
}

fn never_returns(x: number) {
    let y = x + 10
    return true
}

let success = never_returns(100)

print("success:", success)

let a = fn(x: number) { return x * 2 }(test_fn(140, 150))

let a_as_string = core.to_string(a)
core.print("a", "has", "been", "evaluated", "as:", a_as_string, "!")

return mod.subber(mod.adder(a, mod.constant), 10)