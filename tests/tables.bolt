// map types
let c: { ..number: string }*/

// non-identifier table literals
let d = {
    1: true,
    "helo": 10,
    true: fn { print("What!!") },
    null: "erm.. okay"
}

print("------------")
print(d[1])
print(d["helo"])
print(d.helo)
print(d[true])
d[true]()
print(d[null])