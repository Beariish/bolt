import meta
import to_string from core

let constant = 100

type TestType = {
    x: number
}

let test_constant = TestType => { x: 69 }

method TestType.@format {
    return "TestType(" + to_string(this.x) + ")"
}

meta.register_prelude("constant", number, constant)
meta.register_type("TestType", TestType)
meta.register_prelude("test_constant", TestType, test_constant)

export 100 as huh