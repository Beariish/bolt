import * from core 

type Person = {
    name: string,
    age: number,
    children: [{
        name: string,
        age: {
            min: number,
            max: number
        }
    }]
}

let i: Person

print(i.children)

i.children.push({
    name: "Alfred",
    age: { min: 10, max: 20 }
})

print(i.children)

print("min:", i.children[0].age.min, "max:", i.children[0].age.max)

fn anon_arg(a: { name: string, age: number | string }) {
    print("Arg is { name:", a.name, "age:", a.age, "}")

    if a.age is number { print("AGE IS NUMBER") }
    if a.age is string { print("AGE IS STRING") }
}

anon_arg({
    name: "Jonny",
    age: 20
})

anon_arg({
    name: "Albert",
    age: "Far too old..."
})