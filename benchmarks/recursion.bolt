import bench from "bench"

fn fib(n: number): number {
    return match n {
        < 2 then n
        else fib(n - 1) + fib(n - 2) 
    }
}

fn fib_iter(n: number) {
    let x = 0
    let y = 1
    for i in n - 1 {
        let z = x + y
        x = y
        y = z
    }

    return y
}

bench("fib(28)", 25, fn {
    fib(28)
})

bench("fib(30)", 25, fn {
    fib(30)
})

bench("fib_iter(30)", 150, fn {
    fib_iter(30)
})